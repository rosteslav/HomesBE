{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "DetailedErrors": true,
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "PostgresConnectionString": "Host=isabelle.db.elephantsql.com;Port=5432;Database={database};Username={username};Password={password};ApplicationName=BuildingMarket;"
    },
    "RedisConnectionConfig": {
        "Host": "{redishost}",
        "Port": "{port}",
        "Password": "{password}"
    },
    "RedisStoreSettings": {
        "ReportsHashKey": "properties:reports"
    },
    "JWT": {
        "ValidAudience": "http://localhost:4200",
        "ValidIssuer": "http://localhost:5000",
        "ValidHours": 3,
        "Secret": "{jwtsecret}"
    }
}
